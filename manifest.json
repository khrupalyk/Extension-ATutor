{
  "name": "EasyTutor",
  "version": "1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://file/*",
        "*://dl.tntu.edu.ua/*",
        "*://easytutorsimple.esy.es/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*/"
  ],
  "web_accessible_resources": [
    "/jquery.js",
    "/injected.js",
    "/jquery.cookie.js"
  ],
  "icons": { 
    "16": "images/16.png", 
    "48": "images/48.png", 
    "128": "images/128.png"
  }
}
