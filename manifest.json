{
  "name": "EasyTutor-beta",
  "version": "1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://file/*",
        "*://dl.tntu.edu.ua/*",
        "*://easytutorsimple.esy.es/*",
        "http://localhost/*"
      ],
      "js": [
        "jquery.js",
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "http://*/"
  ],
  "web_accessible_resources": [
    "/jquery.js",
    "/injected.js",
    "/jquery.cookie.js"
  ]
}
